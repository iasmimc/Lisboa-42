#include <unistd.h>

void	ft_print_numbers(void)

{
	int	cont;

	cont = '0';
	while (cont <= '9')
	{
		write (1, &cont, 1);
		cont ++;
	}
}

int	main(void)

{
	ft_print_numbers ();
	write(1, "\n", 1);
}
